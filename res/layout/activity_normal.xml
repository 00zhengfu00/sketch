<?xml version="1.0" encoding="utf-8"?>
<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android" style="@style/match_match" android:background="@color/black">
    <LinearLayout style="@style/match_wrap_vertical">
        <LinearLayout style="@style/match_wrap">
            <FrameLayout style="@style/weight_wrap">
                <ImageView
                    android:id="@+id/image_normal_11"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:contentDescription="@string/app_name"/>
                <ProgressBar
                    style="?android:progressBarStyleHorizontal"
                    android:id="@+id/progress_normal_11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>
            </FrameLayout>

            <FrameLayout style="@style/weight_wrap" android:layout_marginLeft="2px">
                <ImageView
                    android:id="@+id/image_normal_12"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:contentDescription="@string/app_name"/>
                <ProgressBar
                    style="?android:progressBarStyleHorizontal"
                    android:id="@+id/progress_normal_12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>
            </FrameLayout>

            <FrameLayout style="@style/weight_wrap" android:layout_marginTop="2px">
                <ImageView
                    android:id="@+id/image_normal_13"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:contentDescription="@string/app_name"/>
                <ProgressBar
                    style="?android:progressBarStyleHorizontal"
                    android:id="@+id/progress_normal_13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout style="@style/match_wrap" android:layout_marginTop="2px">
            <FrameLayout style="@style/wrap_wrap">
                <ImageView
                    android:id="@+id/image_normal_21"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:contentDescription="@string/app_name"/>
                <ProgressBar
                    style="?android:progressBarStyleHorizontal"
                    android:id="@+id/progress_normal_21"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>
            </FrameLayout>

            <FrameLayout style="@style/wrap_wrap" android:layout_marginLeft="2px">
                <ImageView
                    android:id="@+id/image_normal_22"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:contentDescription="@string/app_name"/>
                <ProgressBar
                    style="?android:progressBarStyleHorizontal"
                    android:id="@+id/progress_normal_22"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>
            </FrameLayout>
        </LinearLayout>

        <FrameLayout style="@style/match_match" android:layout_marginTop="2px">
            <ImageView
                android:id="@+id/image_normal_31"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:contentDescription="@string/app_name"/>
            <ProgressBar
                style="?android:progressBarStyleHorizontal"
                android:id="@+id/progress_normal_31"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"/>
        </FrameLayout>
    </LinearLayout>
</ScrollView>
